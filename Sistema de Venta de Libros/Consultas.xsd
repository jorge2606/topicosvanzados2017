                    <CommandText>SELECT        LIB.ISBN, LIB.nombre AS NOMBRE, CAT.descripcion AS CATEGORIA, LIB.precio AS PRECIO, EST.descripcion AS ESTADO, LIB.cantidad AS CANTIDAD, LIB.imagen AS RUTA, LIB.año AS AÑO, 
